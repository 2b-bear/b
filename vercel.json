{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Report-To",
          "value": "{\"group\":\"default\",\"max_age\":10886400,\"endpoints\":[{\"url\":\"https://69fc9597f1a281ba71908dde6b7eaab0.report-uri.com/a/d/g\"}],\"include_subdomains\":true}"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; style-src 'self'; base-uri 'none'; script-src https://static.cloudflareinsights.com;"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), geolocation=(), microphone=(), notifications=(), accelerometer=(), ambient-light-sensor=(), autoplay=(), gyroscope=(), magnetometer=(), payment=(), usb=()"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/about", "destination": "/about.html" },
    { "source": "/credits", "destination": "/credits.html" },
    { "source": "/index", "destination": "/index.txt" }
  ]
}
