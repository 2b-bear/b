{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Report-To",
          "value": "{\"group\":\"default\",\"max_age\":10886400,\"endpoints\":[{\"url\":\"https://69fc9597f1a281ba71908dde6b7eaab0.report-uri.com/a/d/g\"}],\"include_subdomains\":true}"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; style-src 'self'; base-uri 'none'; report-to default; report-uri https://69fc9597f1a281ba71908dde6b7eaab0.report-uri.com/r/d/csp/enforce; script-src https://www.b-bear.org https://static.cloudflareinsights.com;"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), geolocation=(), microphone=(), notifications=(), accelerometer=(), ambient-light-sensor=(), autoplay=(), gyroscope=(), magnetometer=(), payment=(), usb=()"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ],
  "redirects": [
    { "source": "/index.html", "destination": "/", "statusCode": 307 },
    { "source": "/index", "destination": "/", "statusCode": 307 },
    { "source": "/index/", "destination": "/", "statusCode": 307 },
    { "source": "/index.html/", "destination": "/", "statusCode": 307 },
    { "source": "/terms.html", "destination": "/terms", "statusCode": 307 },
    { "source": "/terms", "destination": "/terms", "statusCode": 307 },
    { "source": "/terms/", "destination": "/terms", "statusCode": 307 },
    { "source": "/terms.html/", "destination": "/terms", "statusCode": 307 },
    { "source": "/b.txt", "destination": "/b", "statusCode": 307 },
    { "source": "/b", "destination": "/b", "statusCode": 307 },
    { "source": "/b/", "destination": "/b", "statusCode": 307 },
    { "source": "/b.txt/", "destination": "/b", "statusCode": 307 }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/terms", "destination": "/terms.html" },
    { "source": "/b", "destination": "/b.txt" }
  ]
}
